        { "Title": "置专辑名为 %0",
			"Parameters": [["Text", "输入专辑名"]] },
		  
        { "Title": "使用相机 %0 拍照，标题 %1，描述 %2 ",
			"Parameters": [["Integer", "相机 (0: 后面、 1: 前面)"],
			             ["Text", "输入标题"],
		          	     ["Text", "输入描述"]] },
        
	{ "Title": "使用相机 %0 拍视频",
		"Parameters": [["Integer", "相机 (0: 后面、 1: 前面)"]]  },
		
        { "Title": "选择图库类型为 %0",
			"Parameters": [["Integer", "选择类型 (0: 全部、 1: 照片、 2: 视频)"]] }		  		  	  		  
   ],

    "Conditions":
    [
        {   "Title": "%o : 拍照时",
            "Triggered": true
        },
        {   "Title": "%o : 拍视频时",
