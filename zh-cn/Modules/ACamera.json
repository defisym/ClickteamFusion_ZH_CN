            "Triggered": true
        }	
	],

    "Expressions":
    [
        {   "Title": "Error(",
            "Returns": "Integer"
        },
		{   "Title": "Error$(",
            "Returns": "Text"
        },
		{   "Title": "ImagePath$(",
            "Returns": "Text"
        },
		{   "Title": "VideoPath$(",
            "Returns": "Text"
        },
		{   "Title": "GalleryPath$(",
            "Returns": "Text"
        },
		{   "Title": "NumberOfCameras(",
            "Returns": "Integer"
        },
		{   "Title": "ImageAngle(",
            "Returns": "Integer"
        }
	]
